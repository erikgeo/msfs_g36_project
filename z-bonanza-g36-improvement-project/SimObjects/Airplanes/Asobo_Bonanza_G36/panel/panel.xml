<PlaneHTMLConfig>

	<Instrument>
		<Name>AS1000_PFD</Name>
		<SyntheticVision>True</SyntheticVision>
		<AvionicsKnobIndex>14</AvionicsKnobIndex> <!-- Ensures compatibility with G1000 mod -->
		<Electric>
			<Simvar name="CIRCUIT ON:25" unit="Boolean"/>
		</Electric>
    <ReversionaryMode>True</ReversionaryMode>
	</Instrument>

	<Instrument>
		<Name>AS1000_MFD</Name>
		<Electric>
			<Simvar name="CIRCUIT ON:26" unit="Boolean"/>
		</Electric>
	</Instrument>

  <Logic>
    <Handler>Systems_AS1000</Handler>
    <PFD>AS1000_PFD</PFD>
    <MFD>AS1000_MFD</MFD>
  </Logic>

	<Annunciations>

		<Annunciation>
			<Type>Caution</Type>
			<Text>FUEL QTY LO</Text>
			<Condition>
				<And>
					<Lower>
						<Simvar name="FUEL SELECTED QUANTITY" unit="gallon"/>
						<Constant>13</Constant>
					</Lower>
					<Greater>
						<Simvar name="FUEL SELECTED QUANTITY" unit="gallon"/>
						<Constant>3.1</Constant>
					</Greater>
				</And>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Warning</Type>
			<Text>FUEL QTY LO</Text>
			<Condition>
				<LowerEqual>
					<Simvar name="FUEL SELECTED QUANTITY" unit="gallon"/>
					<Constant>3.1</Constant>
				</LowerEqual>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Warning</Type>
			<Text>FUEL FLOW HI</Text>
			<Condition>
				<Greater>
					<Simvar name="L:XMLVAR_FUEL_FLOW_INDICATED" unit="gph"/>
					<Constant>27.4</Constant>
				</Greater>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Warning</Type>
			<Text>OIL PRESS HI</Text>
			<Condition>
				<Greater>
					<Simvar name="GENERAL ENG OIL PRESSURE:1" unit="psi"/>
					<Constant>100</Constant>
				</Greater>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Warning</Type>
			<Text>OIL PRESS LO</Text>
			<Condition>
				<Lower>
					<Simvar name="GENERAL ENG OIL PRESSURE:1" unit="psi"/>
					<Constant>10</Constant>
				</Lower>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Caution</Type>
			<Text>OIL PRESS LO</Text>
			<Condition>
				<And>
					<Lower>
						<Simvar name="GENERAL ENG OIL PRESSURE:1" unit="psi"/>
						<Constant>30</Constant>
					</Lower>
					<Greater>
						<Simvar name="GENERAL ENG OIL PRESSURE:1" unit="psi"/>
						<Constant>10</Constant>
					</Greater>
				</And>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Warning</Type>
			<Text>OIL TEMP HI</Text>
			<Condition>
				<Greater>
					<Simvar name="ENG OIL TEMPERATURE:1" unit="rankine"/>
					<Constant>700</Constant>
				</Greater>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Warning</Type>
			<Text>CHT HI</Text>
			<Condition>
				<Greater>
					<Simvar name="ENG CYLINDER HEAD TEMPERATURE:1" unit="celsius"/>
					<Constant>238</Constant>
				</Greater>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Warning</Type>
			<Text>GEAR UP</Text>
			<Condition>
				<And>
					<Simvar name="IS GEAR RETRACTABLE" unit="Boolean"/>
					<Not>
						<Simvar name="GEAR HANDLE POSITION" unit="Boolean"/>
					</Not>
					<Or>
						<Greater>
							<Simvar name="FLAPS HANDLE INDEX" unit="number"/>
							<Constant>1</Constant>
						</Greater>
						<Equal>
							<Simvar name="GENERAL ENG THROTTLE LEVER POSITION:1" unit="percent"/>
							<Constant>0</Constant>
						</Equal>
					</Or>
				</And>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Caution</Type>
			<Text>BUS1 VOLT HI</Text>
			<Condition>
			<Greater>
				 <Simvar name="ELECTRICAL MAIN BUS VOLTAGE:1" unit="volts"/>
				 <Constant>30</Constant>
			</Greater>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Caution</Type>
			<Text>BUS1 VOLT LO</Text>
			<Condition>
			<Lower>
				 <Simvar name="ELECTRICAL MAIN BUS VOLTAGE:1" unit="volts"/>
				 <Constant>24</Constant>
			</Lower>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Caution</Type>
			<Text>BUS2 VOLT HI</Text>
			<Condition>
			<Greater>
				 <Simvar name="ELECTRICAL MAIN BUS VOLTAGE:2" unit="volts"/>
				 <Constant>30</Constant>
			</Greater>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Caution</Type>
			<Text>BUS2 VOLT LO</Text>
			<Condition>
			<Lower>
				 <Simvar name="ELECTRICAL MAIN BUS VOLTAGE:2" unit="volts"/>
				 <Constant>24</Constant>
			</Lower>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Caution</Type>
			<Text>ALT 1 LOAD</Text>
			<Condition>
			<Greater>
				 <Simvar name="ELECTRICAL GENALT BUS AMPS:1" unit="amps"/>
				 <Constant>100</Constant>
			</Greater>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Caution</Type>
			<Text>ALT 2 LOAD</Text>
			<Condition>
			<Greater>
				 <Simvar name="ELECTRICAL GENALT BUS AMPS:2" unit="amps"/>
				 <Constant>24</Constant>
			</Greater>
			</Condition>
		</Annunciation>

		<!-- ADD ALT 2 LOAD IF BETWEEN 20 AND 24 AMPS FOR 5+ MIN-->

		<Annunciation>
			<Type>Warning</Type>
			<Text>ALT 1 INOP</Text>
			<Condition>
				<And>
					<Not>
						<Simvar name="GENERAL ENG MASTER ALTERNATOR:1" unit="Boolean"/>
					</Not>
					<Simvar name="GENERAL ENG MASTER ALTERNATOR:2" unit="Boolean"/>
					<Simvar name="ENG COMBUSTION:1" unit="Boolean"/>
				</And>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Warning</Type>
			<Text>ALT 2 INOP</Text>
			<Condition>
				<And>
					<Not>
						<Simvar name="GENERAL ENG MASTER ALTERNATOR:2" unit="Boolean"/>
					</Not>
					<Simvar name="GENERAL ENG MASTER ALTERNATOR:1" unit="Boolean"/>
					<Simvar name="ENG COMBUSTION:1" unit="Boolean"/>
				</And>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Warning</Type>
			<Text>ALT 1-2 INOP</Text>
			<Condition>
				<And>
					<Not>
						<Simvar name="GENERAL ENG MASTER ALTERNATOR:1" unit="Boolean"/>
					</Not>
					<Not>
						<Simvar name="GENERAL ENG MASTER ALTERNATOR:2" unit="Boolean"/>
					</Not>
					<Simvar name="ENG COMBUSTION:1" unit="Boolean"/>
				</And>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Advisory</Type>
			<Text>BUSES TIED</Text>
			<Condition>
				<Simvar name="L:XMLVAR_BUSES_TIED" unit="Boolean"/>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Caution</Type>
			<Text>STARTER ENGD</Text>
			<Condition>
				<And>
					<Simvar name="ENG COMBUSTION:1" unit="Boolean"/>
					<Simvar name="GENERAL ENG STARTER:1" unit="Boolean"/>
				</And>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Caution</Type>
			<Text>PITOT FAIL</Text>
			<Condition>
				<Simvar name="PARTIAL PANEL PITOT" unit="Boolean"/>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Caution</Type>
			<Text>CHECK GEAR</Text>
			<Condition>
				<Or>
					<And>
						<Simvar name="SIM ON GROUND" unit="Boolean"/>
						<Not>
							<Simvar name="GEAR HANDLE POSITION" unit="Boolean"/>
						</Not>
					</And>
					<And>
						<Greater>
							<Simvar name="GEAR CENTER POSITION" unit="Percent"/>
							<Constant>0</Constant>
						</Greater>
						<Lower>
							<Simvar name="GEAR CENTER POSITION" unit="Percent"/>
							<Constant>100</Constant>
						</Lower>
					</And>
					<And>
						<Greater>
							<Simvar name="GEAR LEFT POSITION" unit="Percent"/>
							<Constant>0</Constant>
						</Greater>
						<Lower>
							<Simvar name="GEAR LEFT POSITION" unit="Percent"/>
							<Constant>100</Constant>
						</Lower>
					</And>
					<And>
						<Greater>
							<Simvar name="GEAR RIGHT POSITION" unit="Percent"/>
							<Constant>0</Constant>
						</Greater>
						<Lower>
							<Simvar name="GEAR RIGHT POSITION" unit="Percent"/>
							<Constant>100</Constant>
						</Lower>
					</And>
				</Or>
			</Condition>
		</Annunciation>

		<Annunciation>
				<Type>Caution</Type>
				<Text>PITOT FAIL</Text>
				<Condition>
					<Simvar name="PARTIAL PANEL PITOT" unit="Boolean"/>
				</Condition>
		</Annunciation>

	</Annunciations>

	  <!-- Voices Alerts -->

  <VoicesAlerts>

		<Alert>
			<Type>SoundOnly</Type>
			<SoundEvent>aural_500ft</SoundEvent>
			<Condition>
				<StateMachine>
					<State id="Under" value="0">
						<Transition to="Over">
							<GreaterEqual>
								<Simvar name="PLANE ALT ABOVE GROUND MINUS CG" unit="feet"/>
								<Constant>510</Constant>
							</GreaterEqual>
						</Transition>
					</State>
					<State id="Over" value="0">
						<Transition to="Alerting">
							<LowerEqual>
								<Simvar name="PLANE ALT ABOVE GROUND MINUS CG" unit="feet"/>
								<Constant>500</Constant>
							</LowerEqual>
						</Transition>
					</State>
					<State id="Alerting" value="1">
						<Transition to="Under">
							<Constant>1</Constant>
						</Transition>
					</State>
				</StateMachine>
			</Condition>
		</Alert>

		<Alert>
			<Type>SoundOnly</Type>
			<SoundEvent>aural_stall</SoundEvent>
			<Condition>
				<Simvar name="STALL WARNING" unit="Bool"/>
			</Condition>
		</Alert>

		<Alert>
			<Type>Warning</Type>
			<ShortText>PULL UP</ShortText>
			<LongText>PULL UP</LongText>
			<SoundEvent>aural_pull_up</SoundEvent>
			<Condition>
				<And>
					<LinearMultiPoint>
						<Table>
							<ReferencePoints>1160,2320,4930,12000</ReferencePoints>
							<Maximums>0,1070,2380,4285</Maximums>
						</Table>
						<Param>
							<Multiply>
								<Simvar name="VERTICAL SPEED" unit="feet per minute"/>
								<Constant>-1</Constant>
							</Multiply>
						</Param>
						<Param>
							<Simvar name="PLANE ALT ABOVE GROUND" unit="feet"/>
						</Param>
					</LinearMultiPoint>
					<Not>
						<Simvar name="SIM ON GROUND" unit="bool"/>
					</Not>
				</And>
			</Condition>
		</Alert>

		<Alert>
			<Type>SoundOnly</Type>
			<SoundEvent>aural_overspeed</SoundEvent>
			<Condition>
				<Simvar name="OVERSPEED WARNING" unit="bool"/>
			</Condition>
		</Alert>

		<Alert>
			<Type>Caution</Type>
			<ShortText>TERRAIN</ShortText>
			<LongText>SINK RATE</LongText>
			<SoundEvent>aural_sink_rate</SoundEvent>
			<Condition>
					<And>
						<LinearMultiPoint>
							<Table>
								<ReferencePoints>1160,2320,4930,12000</ReferencePoints>
								<Minimums>0,1070,2380,4285</Minimums>
								<Maximums>0,1460,2980,5360</Maximums>
							</Table>
							<Param>
								<Multiply>
									<Simvar name="VERTICAL SPEED" unit="feet per minute"/>
									<Constant>-1</Constant>
								</Multiply>
							</Param>
							<Param>
								<Simvar name="PLANE ALT ABOVE GROUND" unit="feet"/>
							</Param>
						</LinearMultiPoint>
						<Not>
							<Simvar name="SIM ON GROUND" unit="bool"/>
						</Not>
					</And>
			</Condition>
		</Alert>

		<Alert>
			<Type>SoundOnly</Type>
			<SoundEvent>aural_landing_gear</SoundEvent>
			<Condition>
				<And>
					<Simvar name="IS GEAR RETRACTABLE" unit="Boolean"/>
					<Not>
						<Simvar name="GEAR HANDLE POSITION" unit="Boolean"/>
					</Not>
					<Or>
						<Greater>
							<Simvar name="FLAPS HANDLE INDEX" unit="number"/>
							<Constant>1</Constant>
						</Greater>
						<Equal>
							<Simvar name="GENERAL ENG THROTTLE LEVER POSITION:1" unit="percent"/>
							<Constant>0</Constant>
						</Equal>
					</Or>
				</And>
			</Condition>
		</Alert>

		<Alert>
			<Type>Test</Type>
			<ShortText>TAWS TEST</ShortText>
			<Condition>
				<And>
					<Lower>
						<TimeSinceStart/>
						<Constant>30000</Constant>
					</Lower>
					<InstrumentWasOff/>
				</And>
			</Condition>
		</Alert>

		<Alert>
			<Type>SoundOnly</Type>
			<SoundEvent>aural_taws_system_test_ok</SoundEvent>
			<Condition>
				<And>
					<Greater>
						<TimeSinceStart/>
						<Constant>30000</Constant>
					</Greater>
					<InstrumentWasOff/>
				</And>
			</Condition>
			<Once>True</Once>
		</Alert>

  </VoicesAlerts>

